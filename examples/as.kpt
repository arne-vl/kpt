altij x = 10
altij y = 5

as (just) {
    zegt("das altij just")
}

as (x > y) {
    zegt("das ok just")
}

as (x < y) {
    zegt("das ni just")
}

# aans as == else if
funkse test (getal) {
    as (getal == 1) {
        zegt("jen")
    } aans as (getal == 2) {
        zegt("twjee")
    } aans as (getal == 3) {
        zegt("drei")
    } aans {
        zegt("trekt uwe plan")
    }
}

test(1)
test(2)
test(3)
test(10)